        </section>
      
        <!-- Team-->
       
        <!-- Contact-->
        <div class="container_">
            <a href="https://web.facebook.com/semoncef" ><i class="fab fa-facebook"></i></a>
            <a href="https://www.instagram.com/concierge_vip_marrakech/" ><i class="fab fa-instagram"></i></a>
            <a href="mailto:kechtrans@gmail.com" ><i class="fa fa-envelope"></i></a>
            <input class="rotating" type="checkbox" name="" id="btn">
             </div>


          
          <!--Go Up-->
          <button onclick="topFunction()" id="goUp" title="Go to top">
          
             </button>
          

    </div>
