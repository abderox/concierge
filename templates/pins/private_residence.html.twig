{% extends 'base.html.twig' %}
{% block title %}Private Residence{% endblock %}
{% block body %}
        <link href="css/styles.css" rel="stylesheet" />
        <link rel="stylesheet" href="css/style1.css">
    <style>
        header.masthead{
            padding-top: 10.5rem !important;
            padding-bottom: 6rem !important;
            text-align: center !important;
            color: #fff !important;
            background-repeat: no-repeat !important;
            background-attachment: scroll;
            background-position: center center !important;
            background-size: cover !important;
            background: linear-gradient(to top, transparent 0%, rgba(0, 0, 0, 0.767) 100%),url("../img/trips_resized.jpg");}
        section{
            background-color: #efefef !important;
            background-image: url("https://www.transparenttextures.com/patterns/crisp-paper-ruffles.png") !important;
        }

        .alertt {
            width: 30%;
            margin: 0px auto;
            padding: 30px;
            position: relative;
            border-radius: 5px;
            box-shadow: 0 0 15px 5px #ccc;
            height: 40px;
        }
        h5{
            margin-top: -20px;
            margin-bottom: -25px;
        }

        .close {
            position: absolute;
            width: 20px;
            height: 20px;
            opacity: 0.5;
            right: 15px;
            top: 10px;
            text-align: center;
            font-size: 1.6em;
            cursor: pointer;
        }


        .warning-alert {
            background-color: #ffd48a;
            border-left: 5px solid #8a5700;
        }
        .warning-alert .close {
            border-color: #8a5700;
            color: #8a5700;

        }
        @media (max-width: 1000px)
        {
            .alertt{
                width: 95%;

            }

        }
        .portfolio-link img{
            width:416px;
            height: 278px;
        }

    </style>
    <div id="page-top">
        <!-- Navigation-->

        <!-- Masthead-->
        <header class="masthead">
            <div class="container">
                <div class="masthead-heading text-uppercase">Private Residence</div>
